#!/bin/sh
mv -f /opt/ppsspp/ppsspp.desktop /usr/share/applications/
mv -f /opt/ppsspp/ppsspp.png /usr/share/pixmaps/
set -e
if [ "$1" = "configure" ] && [ -x "`which update-menus 2>/dev/null`" ]; then
	update-menus
fi
chmod +x /opt/ppsspp/PPSSPPSDL
echo ""
echo "
 +-+-+-+-+-+-+-+-+-+-+-+-+
 |N o o b s L a B . c o m|
 +-+-+-+-+-+-+-+-+-+-+-+-+
"
echo ""
echo "keep visit on www.NoobsLab.com"
sleep 2
echo ""

