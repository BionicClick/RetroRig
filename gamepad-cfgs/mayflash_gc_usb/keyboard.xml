<!--
# This file is for use with a Gamecube Controller connected
# via a Mayflash USB adapter under RetroRig
# Button id numbers are totally diffferent, due to the use of
# the use-dpad-as-button, and use-trigger-as-button options used.
# To use please replace the joystickname with the one you find 
# in your xbmclogfile
#
# Be careful:
# I generally encourage people to use the analog stick
# The DPad gets remapped from its default (up, down, etc) to various
# actions (delete, queue, etc.)
# The AnalogStick should be used for browsing.
#
# The Buttons of the controller are mapped as the following list shows:
# add +1, since id's start at 0!
#
#button id 1 = X button
#button id 2 = A button
#button id 3 = B button
#button id 4 = Y button
#button id 5 = Left Shoulder button
#button id 6 = Right Shoulder button
#button id 7 = 
#button id 8 = Z button
#button id 9 = 
#button id 10 = Start button
#button id 11 = 
#button id 12 = 
#button id 13 = dpad up
#button id 14 = dpad right
#button id 15 = dpad down
#button id 16 = dpad left
#
#axis limit="1" id="2"   Up on left stick
#axis limit="+1" id="2"   Down on left stick
#axis limit="1" id="1"   Left on left stick
#axis limit="+1" id="1"   Right on left stick
#
#
#axis limit="1" id="4"   Depress Left Shoulder
#axis limit="+1" id="4"   Increase Left Shoulder
#axis limit="1" id="5"   Depress Left Shoulder
#axis limit="+1" id="5"   Increase Left Shoulder
#
#axis limit="1" id="3"   Up on right stick
#axis limit="+1" id="3"   Down on right stick
#axis limit="1" id="6"   Left on right stick
#axis limit="+1" id="6"   Right on right stick
#
-->
<keymap>
  <global>
    <joystick name="HuiJia  USB GamePad">
<altname>Gamecube Controller (HuiJia  USB GamePad)</altname>
<button id="13">Up</button> 
<button id="15">Down</button> 
<button id="16">Left</button>
<button id="14">Right</button>
<button id="2">Select</button>
<button id="3">Back</button>
<button id="1">ContextMenu</button>
<button id="4">FullScreen</button>
<button id="5">ScrollUp</button>
<button id="6">ScrollDown</button>

<hat id="2" position="up">Up</hat>
<hat id="2" position="down">Down</hat>
<hat id="2" position="right">Right</hat>
<hat id="2" position="left">Left</hat>
  
<axis limit="-1" id="3">VolumeUp</axis>
<axis limit="+1" id="3">VolumeDown</axis>
<axis limit="-1" id="6">AnalogSeekBack</axis>
<axis limit="+1" id="6">AnalogSeekForward</axis> 

    </joystick>
  </global>
